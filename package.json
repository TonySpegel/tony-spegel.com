{
  "name": "tony-spegel.com-website",
  "version": "6.0.0",
  "description": "My personal website. I blog about the modern web.",
  "scripts": {
    "build": "npx @11ty/eleventy",
    "build:prod": "ENVIRONMENT=production npx @11ty/eleventy",
    "bench": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
    "watch": "npx @11ty/eleventy --watch",
    "serve": "npx @11ty/eleventy --serve",
    "start": "npx @11ty/eleventy --serve",
    "debug": "DEBUG=* npx @11ty/eleventy",
    "11ty-wds": "concurrently \"npx eleventy --watch\" \"web-dev-server  --node-resolve\"",
    "build-rollup-live": "rollup --config rollup.config.live.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/TonySpegel/tony-spegel.com.git"
  },
  "author": {
    "name": "Tony Spegel",
    "email": "tony.spegel@gmail.com",
    "url": "https://tony-spegel.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TonySpegel/tony-spegel.com/issues"
  },
  "homepage": "https://github.com/TonySpegel/tony-spegel.com#readme",
  "dependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-navigation": "^0.3.3",
    "@11ty/eleventy-plugin-rss": "^1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@sindresorhus/slugify": "^1.1.2",
    "eleventy-plugin-nesting-toc": "^1.3.0",
    "luxon": "^2.4.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.4",
    "markdown-it-header-sections": "^1.0.0",
    "theme-switch-component": "^0.0.6",
    "toc-observer-component": "^0.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@web/dev-server": "^0.1.32",
    "concurrently": "^7.2.2",
    "rollup": "^2.76.0",
    "rollup-plugin-summary": "^1.4.3",
    "rollup-plugin-terser": "^7.0.2"
  }
}
